{
  "version": 8,
  "metadata": {"maputnik:renderer": "mlgljs"},
  "sources": {
    "openmaptiles": {
      "type": "vector",
      "tiles": ["https://tile.openstreetmap.jp/data/planet/{z}/{x}/{y}.pbf"],
      "attribution": "© <a href=\"https://www.openstreetmap.org/copyright\" target=\"_blank\">OpenStreetMap contributors</a>",
      "maxzoom": 14
    },
    "terrain": {
      "type": "raster-dem",
      "tiles": [
        "https://gbank.gsj.jp/seamless/elev/terrainRGB/land/{z}/{y}/{x}.png"
      ],
      "minzoom": 0,
      "maxzoom": 14,
      "tileSize": 256,
      "encoding": "mapbox",
      "attribution": "<a href=\"https://maps.gsi.go.jp/development/ichiran.html\">地理院タイル</a> | <a href=\"https://gbank.gsj.jp/seamless/elev/\">産総研シームレス標高タイル</a>"
    }
  },
  "glyphs": "https://chizutodesign.github.io/fonts/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "layout": {"visibility": "visible"},
      "paint": {"background-color": "rgba(237, 230, 222, 1)"}
    },
    {
      "id": "landcover-wood",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["all", ["==", "class", "wood"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": {
          "stops": [
            [6, "rgba(237, 230, 222, 1)"],
            [10, "rgba(190, 206, 140, 1)"]
          ]
        },
        "fill-antialias": false
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "water",
      "filter": ["all", ["!=", "intermittent", 1]],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "rgba(102, 170, 204, 1)"}
    },
    {
      "id": "waterway-river",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "waterway",
      "filter": ["any", ["==", "class", "river"], ["!=", "brunnel", "tunnel"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(102, 170, 204, 1)",
        "line-width": {"stops": [[11, 0.5], [20, 6]]}
      }
    },
    {
      "id": "rail",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 10,
      "filter": ["all", ["in", "class", "rail", "transit"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(57, 57, 56, 1)",
        "line-width": 1,
        "line-opacity": {"stops": [[10, 0.1], [16, 0.2]]}
      }
    },
    {
      "id": "road-motorway-blur",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 0,
      "filter": ["all", ["==", "class", "motorway"]],
      "layout": {
        "line-join": "round",
        "line-cap": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "rgba(240, 221, 168, 1)",
        "line-width": 15,
        "line-blur": 25
      }
    },
    {
      "id": "road-path-pedst",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 15,
      "filter": [
        "all",
        [
          "in",
          "class",
          "footway",
          "path",
          "pedestrian",
          "service",
          "track",
          "raceway"
        ]
      ],
      "layout": {
        "line-join": "round",
        "line-cap": "round",
        "visibility": "visible"
      },
      "paint": {"line-color": "#D66F5C", "line-width": 1, "line-opacity": 0.5}
    },
    {
      "id": "road-secotertiary",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 13,
      "filter": [
        "all",
        ["in", "class", "secondary", "tertiary", "minor"],
        ["!=", "brunnel", "tunnel"]
      ],
      "layout": {
        "line-join": "round",
        "line-cap": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#D66F5C",
        "line-width": {"stops": [[12, 0.5], [16, 1]]},
        "line-opacity": 0.5
      }
    },
    {
      "id": "road-trunk-primary",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 10,
      "filter": ["all", ["in", "class", "trunk", "primary"]],
      "layout": {
        "line-join": "round",
        "line-cap": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#D66F5C",
        "line-width": {"base": 2.2, "stops": [[13, 1], [16, 2]]},
        "line-opacity": 0.5
      }
    },
    {
      "id": "road-motorway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 0,
      "filter": ["all", ["==", "class", "motorway"]],
      "layout": {
        "line-join": "round",
        "line-cap": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#D66F5C",
        "line-width": {"base": 2.2, "stops": [[4, 1], [13, 2], [16, 3]]},
        "line-blur": 0,
        "line-opacity": 0.5
      }
    },
    {
      "id": "poi-street",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "minzoom": 14,
      "filter": [
        "all",
        ["in", "class", "hamlet", "islet", "quarter", "island", "neighbourhood"]
      ],
      "layout": {
        "text-font": ["Yuji Syuku Regular"],
        "text-field": ["coalesce", ["get", "name:ja"], ["get", "name"]],
        "text-size": {"stops": [[14, 11], [16, 15]]},
        "text-line-height": 1,
        "text-anchor": "center",
        "text-rotation-alignment": "auto",
        "text-pitch-alignment": "auto",
        "text-writing-mode": ["vertical"]
      },
      "paint": {"icon-color": "rgba(35, 34, 32, 1)"}
    },
    {
      "id": "poi-sta-circle",
      "type": "circle",
      "source": "openmaptiles",
      "source-layer": "poi",
      "minzoom": 11,
      "filter": ["all", ["==", "class", "railway"]],
      "paint": {
        "circle-stroke-color": "rgba(214, 111, 92, 1)",
        "circle-color": "rgba(237, 230, 222, 1)",
        "circle-stroke-opacity": 0.7,
        "circle-stroke-width": 1,
        "circle-opacity": 0,
        "circle-radius": 4
      }
    },
    {
      "id": "poi-sta",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "poi",
      "minzoom": 11,
      "filter": ["all", ["in", "class", "railway"]],
      "layout": {
        "text-font": ["Yuji Syuku Regular"],
        "text-field": ["coalesce", ["get", "name:ja"], ["get", "name"]],
        "text-writing-mode": ["vertical"],
        "text-size": 12,
        "text-line-height": 1,
        "text-anchor": "top-left",
        "icon-anchor": "center",
        "text-justify": "center",
        "text-offset": [0.6, -0.5],
        "text-rotate": 0
      },
      "paint": {"icon-color": "rgba(35, 34, 32, 1)"}
    },
    {
      "id": "poi-ward",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "minzoom": 12,
      "filter": ["all", ["==", "class", "suburb"]],
      "layout": {
        "text-font": ["Yuji Syuku Regular"],
        "text-field": ["coalesce", ["get", "name:ja"], ["get", "name"]],
        "text-size": 13,
        "text-line-height": 1,
        "text-offset": [0, 0],
        "text-anchor": "center",
        "text-writing-mode": ["vertical"]
      },
      "paint": {"icon-color": "rgba(35, 34, 32, 1)"}
    },
    {
      "id": "poi-village",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "minzoom": 8,
      "filter": ["all", ["==", "class", "village"]],
      "layout": {
        "text-font": ["Yuji Syuku Regular"],
        "text-field": ["coalesce", ["get", "name:ja"], ["get", "name"]],
        "text-size": 13,
        "text-line-height": 1,
        "text-offset": [0, 0],
        "text-anchor": "center",
        "text-writing-mode": ["vertical"]
      },
      "paint": {"icon-color": "rgba(35, 34, 32, 1)"}
    },
    {
      "id": "poi-town",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "minzoom": 7,
      "filter": ["all", ["==", "class", "town"]],
      "layout": {
        "text-font": ["Yuji Syuku Regular"],
        "text-field": ["coalesce", ["get", "name:ja"], ["get", "name"]],
        "text-size": 14,
        "text-offset": [0, 0],
        "text-line-height": 1,
        "text-anchor": "center",
        "text-writing-mode": ["vertical"]
      },
      "paint": {"icon-color": "rgba(35, 34, 32, 1)"}
    },
    {
      "id": "poi-city",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "minzoom": 6,
      "maxzoom": 15,
      "filter": ["all", ["==", "class", "city"]],
      "layout": {
        "text-font": ["Yuji Syuku Regular"],
        "text-field": ["coalesce", ["get", "name:ja"], ["get", "name"]],
        "text-size": 16,
        "text-line-height": 1,
        "text-anchor": "center",
        "text-offset": [0, 0],
        "text-writing-mode": ["vertical"]
      },
      "paint": {"icon-color": "rgba(35, 34, 32, 1)"}
    },
    {
      "id": "poi-pref",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "minzoom": 4,
      "maxzoom": 10,
      "filter": ["all", ["in", "class", "province", "state"]],
      "layout": {
        "text-font": ["Yuji Syuku Regular"],
        "text-field": ["coalesce", ["get", "name:ja"], ["get", "name"]],
        "text-size": {"stops": [[4, 16], [8, 20]]},
        "text-line-height": 1,
        "text-anchor": "center",
        "text-offset": [0, 0],
        "text-writing-mode": ["vertical"]
      },
      "paint": {"icon-color": "rgba(35, 34, 32, 1)"}
    },
    {
      "id": "poi-capital",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "maxzoom": 10,
      "filter": ["all", ["==", "capital", 2]],
      "layout": {
        "text-font": ["Yuji Syuku Regular"],
        "text-field": ["coalesce", ["get", "name:ja"], ["get", "name"]],
        "text-size": 20,
        "text-line-height": 1,
        "text-offset": [0, 0],
        "text-anchor": "center",
        "text-writing-mode": ["vertical"]
      },
      "paint": {"icon-color": "rgba(35, 34, 32, 1)"}
    }
  ],
  "localIdeographFontFamily": false,
  "id": "inozu"
}
