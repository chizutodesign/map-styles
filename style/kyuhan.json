    {
      "id": "landcover-wood",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["all", ["==", "class", "wood"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": {"stops": [[6, "#fff"], [10, "rgba(238, 238, 238, 1)"]]},
        "fill-antialias": false
      }
    },
    {
